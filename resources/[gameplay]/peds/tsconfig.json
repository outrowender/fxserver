{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "*": ["types/*"]
    },
    "outDir": "src",
    "noImplicitAny": false,
    "module": "es6",
    "target": "es6",
    "allowJs": false,
    "lib": ["es2017"],
    "types": ["@citizenfx/client","@citizenfx/server", "@types/node"],
    "moduleResolution": "node"
  },
  "include": ["./**/*"],
  "exclude": ["dist"]
}
